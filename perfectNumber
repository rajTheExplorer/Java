class kiara
{
    public static void perfectNumber(int num)
    {
        int sum=0;
        int i=1;
        while(i<num)
        {
            if(num%i==0)
            {
                sum=sum+i;
            }
            i++;

        }
        if(num==sum)
        {
            System.out.println("perfect number");
        }
        else
        {
            System.out.println("non perfect number");
        }
    }

    public static void main(String[] args)
    {
        perfectNumber(28);
    }
}
