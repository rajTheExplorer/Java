class Main
{
    public static void displayingEvenUsingWhile(int num) {
        int i=0;
        while (i <= num) {
            if(i%2==0)
            {
                System.out.println(i);
            }
            i++;
        }
    }

    public static void main(String[] args) {
        displayingEvenUsingWhile(34);
    }

}
