class kiara
{
    public static void isPrime(int num)
    {
        int count=0;
        for(int i=1;i<=num;i++)
        {
            if(num%i==0)
            {
                count++;
            }
        }
        if(count==2)
        {
            System.out.println("prime number");
        }
        else
        {
            System.out.println("not a prime number");
        }
    }
    public static void main(String[] args) {
        isPrime(29);
    }
}
